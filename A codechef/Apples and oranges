#include <bits/stdc++.h>
#define pair pair<int, int>
#define ll long long
#define sort sort(v.begin(), v.end())
#define rev reverse(v.begin(), v.end())
#define yes cout << "YES" << endl
#define no cout << "NO" << endl
#define re return 0

using namespace std;

int main(){
	ll m;
	cin >> m;
	ll a, b;
	cin >> a >> b;
	if(a + b < m) yes;
	else no;
	re;
}


